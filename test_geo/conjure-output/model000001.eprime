language ESSENCE' 1.0

given n: int
letting let1 be n - 1
letting let2 be n - 1
letting let3 be n - 1
find J_FunctionND: matrix indexed by [int(0..let1), int(0..let2)] of int(0..let3)
letting let4 be n - 1
letting let5 be n - 1
letting let6 be n - 1
find M_FunctionND: matrix indexed by [int(0..let4), int(0..let5)] of int(0..let6)
branching on [J_FunctionND, M_FunctionND]
such that
    and([J_FunctionND[i, J_FunctionND[j, k]] = J_FunctionND[J_FunctionND[i, j], k]
             | i : int(0..n - 1), j : int(0..n - 1), k : int(0..n - 1)]),
    and([J_FunctionND[i, j] = J_FunctionND[j, i] | i : int(0..n - 1), j : int(0..n - 1)]),
    and([M_FunctionND[i, j] = M_FunctionND[j, i] | i : int(0..n - 1), j : int(0..n - 1)]),
    and([J_FunctionND[i, i] = i | i : int(0..n - 1)]),
    and([J_FunctionND[M_FunctionND[i, j], i] = i | i : int(0..n - 1), j : int(0..n - 1)]),
    and([J_FunctionND[i, j] = i /\ M_FunctionND[i, j] = j \/ !(J_FunctionND[i, j] = i)
             | i : int(0..n - 1), j : int(0..n - 1)]),
    and([M_FunctionND[n - 1, i] = i | i : int(0..n - 1)]),
    and([J_FunctionND[0, i] = i | i : int(0..n - 1)]),
    and([J_FunctionND[i, j] = i /\ i >= j \/ J_FunctionND[i, j] = j /\ j >= i \/ M_FunctionND[i, j] = 0 \/
         and([J_FunctionND[i, k] = i /\ J_FunctionND[j, l] = j /\ i > j /\ k >= l \/
              J_FunctionND[i, k] = i /\ J_FunctionND[j, l] = j /\ i > j /\ k <= l
              \/ !(J_FunctionND[i, k] = i /\ J_FunctionND[j, l] = j)
                  | k : int(0..n - 1), l : int(0..n - 1)])
             | i : int(0..n - 1), j : int(0..n - 1)]),
    and([J_FunctionND[j, k] < i /\ i < k /\ k < M_FunctionND[i, j] /\
         or([J_FunctionND[j, k] < d /\ d <= j /\ i = J_FunctionND[M_FunctionND[i, d], k] | d : int(0..n - 1)])
         \/ !(J_FunctionND[j, k] < i /\ i < k /\ k < M_FunctionND[i, j])
             | i : int(0..n - 1), j : int(0..n - 1), k : int(0..n - 1)]),
    and([i = 0 \/ !or([M_FunctionND[i, j] = j /\ !(i = j) /\ !(j = 0) | j : int(0..n - 1)]) \/
         or([J_FunctionND[j, k] = i /\ !(j = i) /\ !(k = i) | j : int(0..n - 1), k : int(0..n - 1)])
             | i : int(0..n - 1)])

